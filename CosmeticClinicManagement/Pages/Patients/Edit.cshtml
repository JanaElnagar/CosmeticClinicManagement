@page
@using CosmeticClinicManagement.Localization
@using Microsoft.AspNetCore.Mvc.Localization
@model CosmeticClinicManagement.Pages.Patients.EditModel
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@inject IHtmlLocalizer<CosmeticClinicManagementResource> L


@{
	Layout = null;
}

<abp-dynamic-form abp-model="Patient" asp-page="/Patients/Edit">
	<abp-modal>
		<abp-modal-header title="@Model.Patient.FirstName"></abp-modal-header>
		<abp-modal-body>
			<abp-input asp-for="Id" />
			<abp-form-content />
		</abp-modal-body>
		<abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
	</abp-modal>
</abp-dynamic-form>