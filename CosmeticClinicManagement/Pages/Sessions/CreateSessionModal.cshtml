@page
@using Microsoft.AspNetCore.Mvc.Localization
@using CosmeticClinicManagement.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CosmeticClinicManagement.Pages.Sessions.CreateSessionModalModel
@inject IHtmlLocalizer<CosmeticClinicManagementResource> L
@{
 Layout = null;
}
<abp-dynamic-form abp-model="Session"
 asp-page="/Sessions/CreateSessionModal">
 <abp-modal>
 <abp-modal-header title="@L["NewSession"].Value"></abp-modal-header>
 <abp-modal-body>
      <abp-input asp-for="Session.PlanId" type="hidden" />
 <abp-form-content />
 </abp-modal-body>
 <abp-modal-footer buttons="@(AbpModalButtons.
Cancel|AbpModalButtons.Save)"></abp-modal-footer>
 </abp-modal>
</abp-dynamic-form>