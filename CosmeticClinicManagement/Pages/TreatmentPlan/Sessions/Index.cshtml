@page
@using CosmeticClinicManagement.Pages.TreatmentPlan.Sessions
@using Microsoft.Extensions.Localization
@using CosmeticClinicManagement.Localization
@model IndexModel
@inject IStringLocalizer<CosmeticClinicManagementResource> L
@section scripts
{
 <abp-script src="/Pages/TreatmentPlan/Sessions/Index.js" />
}
<abp-card>
 <abp-card-header>
        <abp-row>
        <abp-column size-md="_6">
        <abp-card-title>@L["Menu:SessionsForTreatmentPlan"]</abp-card-title>
        </abp-column>
        <abp-column size-md="_6" class="text-end">
        <abp-button id="NewSessionButton"
        text="@L["NewSession"].Value"
        icon="plus"
        button-type="Primary"/>
        </abp-column>
        </abp-row>

 </abp-card-header>
 <abp-card-body>
 <abp-table id="SessionsForPlanTable" striped-rows="true" />
 </abp-card-body>
</abp-card> 