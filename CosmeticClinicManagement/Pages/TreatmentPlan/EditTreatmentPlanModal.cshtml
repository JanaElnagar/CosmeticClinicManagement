@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CosmeticClinicManagement.Pages.TreatmentPlan.EditTreatmentPlanModalModel
@inject IHtmlLocalizer<CosmeticClinicManagementResource> L
@{
 Layout = null;
}
<abp-dynamic-form abp-model="TreatmentPlan"
 asp-page="/TreatmentPlan/EditTreatmentPlanModal">
 <abp-modal>
 <abp-modal-header title="@Model.TreatmentPlan.Id"></abp-modal-header>
 <abp-modal-body>
